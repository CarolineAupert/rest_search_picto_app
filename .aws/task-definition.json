{
    "family": "PictoSketchnoteFamily",
    "containerDefinitions": [
        {
            "name": "pictosketchnote",
            "image": "338421022085.dkr.ecr.eu-west-3.amazonaws.com/caukaro:3c9d9ff852180acccba05e73fe352ca04b74fa75",
            "cpu": 0,
            "memory": 512,
            "portMappings": [
                {
                    "name": "pictosketchnote-80-tcp",
                    "containerPort": 8080,
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "SPRING_PROFILES_ACTIVE",
                    "value": "prod"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "pictosketchnote",
                    "containerPath": "/var/www/html",
                    "readOnly": true
                },
                {
                    "sourceVolume": "awseb-logs-pictosketchnote",
                    "containerPath": "/var/log/pictosketchnote-app",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "SPRING_DATASOURCE_URL",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:338421022085:secret:prod/PictoSketchnote/Postgres-0D5LwR:SPRING_DATASOURCE_URL::"
                }, 
                {
                    "name": "SPRING_DATASOURCE_USERNAME",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:338421022085:secret:prod/PictoSketchnote/Postgres-0D5LwR:SPRING_DATASOURCE_USERNAME::"
                }, 
                {
                    "name": "SPRING_DATASOURCE_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:338421022085:secret:prod/PictoSketchnote/Postgres-0D5LwR:SPRING_DATASOURCE_PASSWORD::"
                }, 
                {
                    "name": "CORS_ORIGINS",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:338421022085:secret:prod/PictoSketchnote/Postgres-0D5LwR:CORS_ORIGINS::"
                }, 
                {
                    "name": "LUCENE_DIRECRORY",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:338421022085:secret:prod/PictoSketchnote/Postgres-0D5LwR:LUCENE_DIRECRORY::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/picto-sketchnote",
                    "awslogs-create-group": "true",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::338421022085:role/ecsTaskExecutionRole",
    "volumes": [
        {
            "name": "pictosketchnote",
            "host": {
                "sourcePath": "/var/app/current/pictosketchnote-app"
            }
        },
        {
            "name": "awseb-logs-pictosketchnote",
            "host": {
                "sourcePath": "/var/log/containers/pictosketchnote"
            }
        }
    ]
}
